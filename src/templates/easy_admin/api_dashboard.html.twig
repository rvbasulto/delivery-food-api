{% extends '@EasyAdmin/default/layout.html.twig' %}
{% set _content_title = 'api-platform' |trans %}
{% block page_title %}
    {{ _content_title }}
{% endblock %}
{% block content_header %}
    <h1 class="title">{{ _content_title }}</h1>
{% endblock %}
{% block content %}
    <div style="visibility: hidden;">{{ _content_title }}</div>
    <iframe style="border: 0; width: 100%; height: 1000px" src="{{ api_doc_url }}"  allowfullscreen></iframe>
{% endblock %}